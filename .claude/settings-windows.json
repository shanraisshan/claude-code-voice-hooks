    ],
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "python .claude/hooks/scripts/hooks.py",
            "timeout": 5000,
            "async": true,
            "once": true
          }
        ]
      }
    ],
    "SessionEnd": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "python .claude/hooks/scripts/hooks.py",
            "timeout": 5000,
            "async": true,
            "once": true
          }
        ]
      }
    ],
    "Setup": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "python .claude/hooks/scripts/hooks.py",
            "timeout": 30000,
            "async": true
          }
        ]
      }
